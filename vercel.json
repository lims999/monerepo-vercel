{
  "version": 2,
  "builds": [
    {
      "src": "apps/resume-display/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/resume-editor/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/editor/(.*)",
      "destination": "/apps/resume-editor/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/apps/resume-display/index.html"
    }
  ]
}
