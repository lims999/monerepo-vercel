{
  "version": 2,
  "builds": [
    {
      "src": "apps/resume-display/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/resume-editor/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/resume-editor/(.*)",
      "dest": "/apps/resume-editor/index.html"
    },
    { "handle": "filesystem" },
    {
      "src": "/resume-display/(.*)",
      "dest": "/apps/resume-display/index.html"
    }
  ]
}
